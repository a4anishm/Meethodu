<%#
# To change this template, choose Tools | Templates
# and open the template in the editor.
%>

<fieldset>
<legend id="mee_legend">My Projects</legend>
    <% if @my_projects.empty? %>
    <p>No Projects to display. Please add projects by links on top of this page</p>
    <% else %>
    <% @my_projects.each do |my_project|%>
    <p><%= link_to "Name: #{my_project.name} \n Summary:  #{my_project.summary}", my_project%></p>
    <br></br>
    <% end %>
    <% end %>
</fieldset>
